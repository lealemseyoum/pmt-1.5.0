            <doc:detail>The position of chrominance components in relation to the luminance component.</doc:detail>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="YCbCrSubSampling" type="vuint16">
        <xsd:annotation>
          <xsd:documentation>
            <doc:brief>The sampling ratio of chrominance components in relation to the luminance component.</doc:brief>
            <doc:detail>The sampling ratio of chrominance components in relation to the luminance component.</doc:detail>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
