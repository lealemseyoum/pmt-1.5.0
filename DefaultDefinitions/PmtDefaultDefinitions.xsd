      <xsd:element name="SourceImageDirectory" type="vuint8">
        <xsd:annotation>
          <xsd:documentation>
            <doc:brief>This field contains a Unicode string which records the Sources Image Directory.</doc:brief>
            <doc:detail>This field contains a Unicode string which records the Sources Image Directory.</doc:detail>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SourceImageFileName" type="vuint8">
        <xsd:annotation>
          <xsd:documentation>
            <doc:brief>This field contains a Unicode string which records the Sources Image File Name.</doc:brief>
            <doc:detail>This field contains a Unicode string which records the Sources Image File Name.</doc:detail>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SourceImageVolumeName" type="vuint8">
